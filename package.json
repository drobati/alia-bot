        "@babel/core": "^7.16.0",
        "@sentry/integrations": "^7.114.0",
        "@types/bunyan": "^1.8.11",
        "@types/common-tags": "^1.8.4",
        "@types/config": "^3.3.3",
        "@types/jest": "^29.5.10",
        "@types/node": "^20.9.3",
        "@types/qrcode": "^1.5.5",
        "@typescript-eslint/eslint-plugin": "^6.12.0",
        "@typescript-eslint/parser": "^6.12.0",
        "babel-preset-env": "^0.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.2.0",
        "eslint-plugin-jest": "^27.6.0",
        "jest": "^29.7.0",
        "jest-junit": "^13.0.0",
