{
    "name": "alia-bot",
    "version": "1.0.0",
    "description": "Bot for Arrakis Discord",
    "main": "index.ts",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "lint": "eslint .",
        "test": "cross-env NODE_ENV=test jest",
        "coverage": "cross-env NODE_ENV=test jest --collectCoverage --",
        "commands:detect": "node scripts/discord-commands/command-detector.js",
        "commands:deploy": "node scripts/deploy-commands.js",
        "commands:deploy:staging": "NODE_ENV=staging node scripts/deploy-commands.js --staging",
        "commands:rollback": "node scripts/discord-commands/rollback-commands.js",
        "commands:monitor": "node scripts/discord-commands/deployment-monitor.js",
        "commands:monitor:continuous": "node scripts/discord-commands/deployment-monitor.js --continuous",
        "commands:health": "node scripts/discord-commands/deployment-monitor.js",
        "commands:backup:list": "node scripts/discord-commands/rollback-commands.js --list"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/desert-planet/alia.git"
    },
    "keywords": [
        "bot",
        "discord",
        "alia"
    ],
    "author": "derek.robati@gmail.com",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/desert-planet/alia/issues"
    },
    "homepage": "https://github.com/desert-planet/alia#readme",
    "dependencies": {
        "@discordjs/voice": "^0.19.0",
        "@hapi/hapi": "^21.3.2",
        "@sentry/node": "^10.8.0",
        "@sentry/profiling-node": "^10.8.0",
        "@snazzah/davey": "^0.1.6",
        "@types/jimp": "^0.2.1",
        "@types/node-cron": "^3.0.11",
        "axios": "^1.11.0",
        "bunyan": "^1.8.15",
        "canvas": "^3.2.0",
        "chartjs-node-canvas": "^5.0.0",
        "common-tags": "^1.8.2",
        "config": "^4.1.1",
        "discord.js": "^14.13.0",
        "dotenv": "^10.0.0",
        "ffmpeg-static": "^5.2.0",
        "fuse.js": "^6.4.6",
        "jimp": "^1.6.0",
        "js-yaml": "^4.1.0",
        "lodash": "^4.17.21",
        "mysql2": "^3.14.3",
        "natural": "^6.10.4",
        "node-cron": "^4.2.1",
        "openai": "^4.20.1",
        "prettier": "^2.4.1",
        "qrcode": "^1.4.4",
        "roll": "^1.3.1",
        "sequelize": "^6.29.0",
        "uws": "^200.0.0",
        "yup": "^0.32.11"
    },
